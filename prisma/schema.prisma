generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Note {
  id      Int    @unique @default(autoincrement())
  name    String @unique
  message String
  created_by User? @relation(fields: [created_by_id],references: [id])
  created_by_id Int
}

model User {
  id       Int    @unique @default(autoincrement())
  name     String
  email    String
  password String
  note Note[]
}
