generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Note {
  id      Int    @unique @default(autoincrement())
  name    String @unique
  message String
}

model User {
  id       Int    @unique @default(autoincrement())
  name     String
  email    String
  password String
}
